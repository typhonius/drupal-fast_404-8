<?php

/**
 * Implementation of hook_enable().
 */
function light_not_found_enable() {
  $bootstrap = 1;
  $weight = -1000;
  $type = 'module';
  $name = 'fast_404';
  db_query("UPDATE {system} SET bootstrap = %d, weight = %d WHERE name = '%s' AND type = '%s'", $bootstrap, $weight, $name, $type); 
}
